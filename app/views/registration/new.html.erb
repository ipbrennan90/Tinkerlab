<%= form_for @user, url: sign_up_path do |f| %>
  <%= f.label :username %>
  <%= f.text_area :username %>
  <%= f.label :email %>
  <%= f.text_area :email %>
  <%= f.label :password %>
  <%= f.text_area :password %>
  <%= f.label :password_confirmation %>
  <%= f.text_area :password_confirmation %>
  <%= f.submit "Sign Up" %>
<% end %>
